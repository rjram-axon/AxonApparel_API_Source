       CommProdPrintFlg = @Html.Raw(Json.Encode(ViewBag.CommProdPrintFlg))
        $('#txtFromDate').datepicker({
            dateFormat: 'dd/mm/yy',
            onSelect: function (dateString, txtDate) {
                DtChk = true;
                var companyid = $('#ddlMCompany').val();
                var FDate = $('#txtFromDate').val();
                var TDate = $('#txtToDate').val();
                LoadDDL(companyid, FDate, TDate);
            }
        });
        $('#txtToDate').datepicker({
            dateFormat: 'dd/mm/yy',
            onSelect: function (dateString, txtDate) {
                DtChk = true;
                var companyid = $('#ddlMCompany').val();
                var FDate = $('#txtFromDate').val();
                var TDate = $('#txtToDate').val();
                LoadDDL(companyid, FDate, TDate);
            }
        });
        $('#txtissDate').datepicker({ dateFormat: 'dd/mm/yy' });
    });
</script>



