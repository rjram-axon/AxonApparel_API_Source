       PanelProcessReturnPrintFlg = @Html.Raw(Json.Encode(ViewBag.PanelProcessReturnPrintFlg))

        $('#txtFromDate').datepicker({
            dateFormat: 'dd/mm/yy',
            onSelect: function (dateString, txtDate) {
                DtChk = true;
                var FDate = $('#txtFromDate').val();
                var TDate = $('#txtToDate').val();
                 var companyid = $('#ddlRCompany').val();
                LoadDDL(companyid, OrderType, InterExter, FDate, TDate);
            }
        });
        $('#txtToDate').datepicker({
            dateFormat: 'dd/mm/yy',
            onSelect: function (dateString, txtDate) {
                DtChk = true;
                var FDate = $('#txtFromDate').val();
                var TDate = $('#txtToDate').val();
                var companyid = $('#ddlRCompany').val();
                LoadDDL(companyid, OrderType, InterExter, FDate, TDate);
            }
        });
        $('#txtReturnDate').datepicker({ dateFormat: 'dd/mm/yy' }); 
        $('#txtCancelrefDate').datepicker({ dateFormat: 'dd/mm/yy' }); 
        $('#txtCancelDate').datepicker({ dateFormat: 'dd/mm/yy' });
    });
</script>

