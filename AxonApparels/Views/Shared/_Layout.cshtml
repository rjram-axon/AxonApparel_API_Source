               //type: "POST",
               type: "GET",
               contentType: "application/json;charset=utf-8",
               dataType: "json",
               success: function (result) {
                   debugger;

                   // alert(MnuName);
                   var obj = result.RoleDetList;
                   debugger;
                   // alert(MenuName[j]);
                   var i = 0;
                   var ADD = null;
                   var EDIT = null;
                   var DELETE = null;
                   var PRINT = null;
                   var ALL = null;
                   var MENU = null;

                   hasRights = false;
                              

                   if (username.toUpperCase() != "SUPERUSER") {
                       for (i = 0; i < obj.length; i++) {

                           MENU = obj[i].MenuName;

                           var MenuTandA = MENU.substring(0, 5);

                           if (MenuTandA == "TandA") {

                               if (obj[i].AllFlg != 0) {
                                   ALL = "Y";
                               } else {
                                   ALL = "N";
                               }
                               if (obj[i].AddFlg != 0) {
                                   ADD = "Y";
                               } else {
                                   ADD = "N";
                               }
                               if (obj[i].DelFlg != 0) {
                                   DELETE = "Y";
                               } else {
                                   DELETE = "N";
                               }
                               if (obj[i].EditFlg != 0) {
                                   EDIT = "Y";
                               } else {
                                   EDIT = "N";
                               }
                               if (obj[i].PrintFlg != 0) {
                                   PRINT = "Y";
                               } else {
                                   PRINT = "N";
                               }

                               if (ALL == "Y" || ADD == "Y" || EDIT == "Y" || DELETE == "Y" || PRINT == "Y") {
                                   $("#" + MENU).show();

                                   if (MenuTandA == "TandA") {
                                       hasRights = true;
                                   }
                               }
                               else {
                                   $("#" + MENU).hide();

                                   if (MenuTandA == "TandA") {
                                       hasRights = false;
                                   }
                               }

                               ADD = "N";
                               EDIT = "N";
                               DELETE = "N";
                               PRINT = "N";
                               ALL = "N";
                           }
                       }
                   }
