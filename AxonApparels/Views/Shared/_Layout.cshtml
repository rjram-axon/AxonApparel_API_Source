         function CheckRoleRights() {
             debugger;

             var url = "http://localhost:8087/Dashboard/LoginIndex?UserId=@Request.RequestContext.HttpContext.Session["UserID"]";   //Here it is controllername/ActionName           
           var username = "@Request.RequestContext.HttpContext.Session["UserName"]";

