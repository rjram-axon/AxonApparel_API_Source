        var AllPopupList=[];
        var Trigger=0;
        $(document).ready(function () {
          
            $('.select2').select2();
            Popup_Alert();
            //window.setInterval(Popup_Alert(), 10);  
        });

        setInterval(function() {
           
            var val=localStorage.getItem('Trig');
            if(val==1){
                Trigger=1;
            }
            if(Trigger == 1){
                Popup_Alert()
                Trigger=0;
            }
        },1*( 30*1000));

        function PopupStatusUpdate(){
            if(AllPopupList.length>0){
                var obj={
                    PopupAlert:AllPopupList,
                }
                $.ajax({
                    url:'/UserEntryLog/UpdatePopupStatus',
                    data:JSON.stringify(obj),
                    type: "POST",
                    contentType: "application/json;charset=utf-8",
                    dataType: "json",
                    success: function (result) {
                        $('.TodayNotify').empty();
                        AllPopupList=[];
                        Popup_Alert();
                    }
                });
            }
        }
        function ChildPopup_Alert(newArrey){
            var Notlen=newArrey.length;
            var llen = Notlen-1;
            var id=newArrey[llen]["Id"];
            for (var l = 0; l < newArrey.length; l++) {
                //setInterval(showToastPosition('bottom-right',newArrey[l]["Message"]),300);
                setTimeout(function(y) {    
                    showToastPosition('bottom-right',newArrey[y]["Message"]);
                    localStorage.setItem('NotificId',newArrey[y]["Id"]);
                    if(id==newArrey[y]["Id"]){
                        localStorage.setItem('Trig',1);
                    }
                }, l * 3 * 1020, l); // we're passing x
                    
            }
            
        }
        function Popup_Alert(){
            $.ajax({
                url:'/UserEntryLog/GetPopupAlert',
                contentType:'application/json;charset=utf-8',
                type:'POST',
                dataType:'json',
                success:function(json){
                   
                   
                    var names = json.Value;

                    var lenChk=[];
                    $.each(names, function (i) {
                        if(names[i]["Status"]==false){
                            lenChk.push(names[i]);
                        }
                        
                    });
                    var Notificlen=lenChk.length;
                    var lastlen = Notificlen-1;
                    if(lenChk.length>0){
                    
                        var lastValue=lenChk[lastlen]["Id"];
                    }else{
                        var lastValue=0;
                    }

                    if(lastValue>0){
                        $('#NotificCount').show();
                    }else{
                        $('#NotificCount').hide();
                    }
                    $('#NotificCount').val(Notificlen).text(Notificlen);


                    var value=localStorage.getItem('NotificId');
                    if(value==null || value=='null'){
                        localStorage.setItem('NotificId',lastValue);
                    }else{
                        var newArrey=[];
                        if(names.length>0){
                            for (var l = 0; l < names.length; l++) {
                                if(value < names[l]["Id"]){
                                    if(names[l]["Status"]==false){
                                    
                                        newArrey.push(names[l]);
                                    }
                                }
                            }
                        }
                        if(newArrey.length>0){
                            localStorage.setItem('Trig',0);
                            ChildPopup_Alert(newArrey);
                        }else{
                            localStorage.setItem('Trig',1);
                        }
                    }

                    $('.TodayNotify').empty();
                    if(names.length>0){
                        for (var l = 0; l < names.length; l++) {
                       
                            if(names[l]["Status"]==false){
                                $('.TodayNotify').append('<li>'+
                                                         '<a href="javascript:void(0);">' +
                                                         //'<div class="icon-circle '+ NotifyList["Table"][l].MessageColor +'">' +
                                                         //'<i class="material-icons">info</i>'+
                                                         //'</div>'+
                                                         '<div class="menu-info">'+
                                                         '<h6 style="max-width: 283px;color:#002E8C">' + names[l]["Message"] + '</h6>' +
                                                         //'<p>'+
                                                         //'<i class="material-icons">access_time</i>Today</p>' +
                                                         '</div></a></li>');
                                AllPopupList.push(names[l]);
                            }
                        }
                    }
                   
                }
            });
        }
    </script>
