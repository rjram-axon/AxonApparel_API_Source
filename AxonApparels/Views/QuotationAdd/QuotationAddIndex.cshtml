    section {
        position: relative;
        margin: 0px 0px;
        transition: 0.5s ease;
    }
    .main-container {
    position:relative;
    }
    .tab-menu {
    color:lightblue;
    list-style:none;
    max-width:627px;
    padding:4px;
    white-space:nowrap;
    border-bottom: 1px solid;
    border-top: 1px solid;
    border-radius:50px;
    box-shadow:initial;
    overflow-x:auto;
    user-select:none;
    scroll-behavior:smooth;
    }
        .tab-menu::-webkit-scrollbar {
            display:none;
        }
    .tab-btn {
    color:black;
    display:inline-block;
    font-size:1em;
    font-weight:400;
    margin:0 2px;
    padding:5px 20px;
    border-radius:30px;
    cursor:pointer;
    user-select:none;
    transition:0.3s ease;
    }
    .tab-btn:hover {
        background-color:lightgrey;
    }
    .tab-btn.active {
        background-color:lightblue;
    }
    .tab-nav-bar {
    /*position:relative;*/
    /*padding:65px 10px 40px 10px;*/
    }
    .tab-navigation {
    position:relative;
    /*display:flex;*/
    justify-content:center;
    align-items:center;
    max-width:initial;
    margin:0 auto;
    }
    .left-btn, .right-btn {
    position:absolute;
    color:darkblue;
    font-size:1.8em;
    padding:2px 7px;
    cursor:pointer;
    }
    .left-btn {
    left:0;
    background:linear-gradient(90deg,#a3ddd8,transparent);
    border-top-left-radius:30px;
    border-bottom-left-radius:30px;
    display:none;
    margin-right:2px;
    }
    .right-btn {
    right:0;
    background:linear-gradient(270deg,#a3ddd8,transparent);
    border-top-right-radius:30px;
    border-bottom-right-radius:30px;
    margin-left:2px;
    }
    .tab-content {
    
    width:100%;
    }
    .tab {
    max-width:100%;
    padding:0px 0px;
    top:0;
    bottom:0;
    right:auto;
    left:auto;
    transform:translateX(25px);
    content-visibility:hidden;
    opacity:0;
    }
        .tab.active {
    transform:translateX(0);
    content-visibility:visible;
    opacity:1;
        }
        .tab.tab_row {
        width:100%;
        display:flex;
        justify-content:center;
        aligh_items:center;
        margin:50px 0;
        gap:30px;
        }
</style>
<script>
    $(document).ready(function () {

        var btnleft = document.querySelector(".left-btn");
        var btnright = document.querySelector(".right-btn");
        var tabmenu = document.querySelector(".tab-menu");
        //btnright.addEventListener('click',() => {
        //    tabmenu.sc
        //});
        debugger;
        $(document).on('click', '.right-btn', function () {
            tabmenu.scrollLeft += 150;
            //IconVisibl();
            setTimeout(() => IconVisibl(),50);
        });
        $(document).on('click', '.left-btn', function () {
            tabmenu.scrollLeft -= 150;
            //IconVisibl();
            setTimeout(() => IconVisibl(),50);
        });

        //    var activeDrag=false;
        //    tabmenu.addEventListener("mousemove",(drag) => {
        //        if(!activeDrag)return;
        //    tabmenu.scrollLeft -=drag.movementX;
        //});
        //tabmenu.addEventListener("mousedown",() =>{
        //    activeDrag=true;
        //});
        var tabs=document.querySelectorAll(".tab")
        var tabBtns=document.querySelectorAll(".tab-btn")
        var tab_nav= function(tabBtnClick){
            tabBtns.forEach((tabBtn)=>{
                tabBtn.classList.remove("active");

        });
        tabs.forEach((tab)=>{
            tab.classList.remove("active");
    });
    tabBtns[tabBtnClick].classList.add("active");
    tabs[tabBtnClick].classList.add("active");
    }
    tabBtns.forEach((tabBtn,i)=>{
        tabBtn.addEventListener("click",()=>{
            tab_nav(i);
    });
    });
    });
    window.onload = function(){
        var btnleft = document.querySelector(".left-btn");
        var btnright = document.querySelector(".right-btn");
        var tabmenu = document.querySelector(".tab-menu");
        btnright.style.display=tabmenu.scrollWidth > tabmenu.clientWidth || tabmenu.scrollWidth >= window.innerWidth ? "block":"none";
        btnleft.style.display=tabmenu.scrollWidth >= window.innerWidth ? "":"none";
    }
    window.onresize = function(){
        var btnleft = document.querySelector(".left-btn");
        var btnright = document.querySelector(".right-btn");
        var tabmenu = document.querySelector(".tab-menu");
        btnright.style.display=tabmenu.scrollWidth > tabmenu.clientWidth || tabmenu.scrollWidth >= window.innerWidth ? "block":"none";
        btnleft.style.display=tabmenu.scrollWidth >= window.innerWidth ? "":"none";

        var scrollLeftValue = Math.round(tabmenu.scrollLeft);

        btnleft.style.display = scrollLeftValue > 0?"block":"none";
    }
    function IconVisibl() {
        var btnleft = document.querySelector(".left-btn");
        var btnright = document.querySelector(".right-btn");
        var tabmenu = document.querySelector(".tab-menu");

        var scrollLeftValue = Math.ceil(tabmenu.scrollLeft);
        //console.log("1.", scrollLeftValue);

        var scrollableWidth = tabmenu.scrollWidth - tabmenu.clientWidth;
        //console.log("2.", scrollableWidth);

        btnleft.style.display = scrollLeftValue > 0 ? "block" : "none";
        btnright.style.display = scrollableWidth > scrollLeftValue ? "block" : "none";
    }
</script>
